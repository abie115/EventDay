@model IEnumerable<EventDay.Models.UserContact>

@{
    ViewBag.Title = "Kontakty";
}

<h2>Kontakty</h2>

<table>
    <tr>
        <th>
            Użytkownik
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.UserMember.UserName, "Details", new { name = item.UserMember.UserName })
        </td>
        <td>
            @Html.ActionLink("Usuń kontakt", "DeleteContact", new { id=item.UserContactId })
        </td>
    </tr>
}

</table>
